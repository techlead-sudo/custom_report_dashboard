<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.module.category" id="module_category_custom_dashboard">
            <field name="name">Custom Dashboard</field>
            <field name="description">Access levels for the Custom Dashboard module</field>
            <field name="sequence">10</field>
